{
  "compilerOptions": {
    "baseUrl": "src/",
    "sourceMap": false,
    "paths": {
      "components/*": ["src/components/*"],
      "assets/*": ["src/assets/*"],
      "hooks/*": ["src/hooks/*"],
      "pages/*": ["src/pages/*"],
      "stores/*": ["src/stores/*"],
      "utils/*": ["src/utils/*"],
      "libs/*": ["src/libs/*"],
      "App": ["src/App.js"]
    },
    "strict": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
